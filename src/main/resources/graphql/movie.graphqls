input MovieInput{
    title:String,
    distributor:String,
    year:String,
    type:String
}
type Movie{
    id:ID,
    title:String,
    distributor:String,
    year:String,
    type:String,
    evaluation: Int
}
type Query{
    findMovieById(id:Int):Movie,
    findByTitle(title:String):Movie,
    findAllMovies:[Movie]
}

type Mutation{
    createMovie(input:MovieInput):Movie,
    updateMovie(input:MovieInput):Movie,
    deleteMovieById(id:Int):Boolean
}