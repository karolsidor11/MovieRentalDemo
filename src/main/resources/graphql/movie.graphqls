type Movie{
    id:ID,
    title:String,
    author:String,
    year:String,
    type:String,
    evaluation: Int
}
type Query{
    findMovieById(id:Int):Movie,
    findByTitle(title:String):Movie,
    findAllMovies:[Movie]
}

type Mutation{
    createMovie(Movie:String):Movie,
    updateMovie(Movie:String):Movie,
    deleteMovieById(id:Int):Boolean
}